<section id="events" class="container">
    <h1>All Events</h1>

{{#each events}}
<div class="event">
      <h3>{{title}}</h3>
      <p>{{description}}</p>
      <ul>
        <li>{{details}}</li>
        <li><a href="mailto:{{contact_email}}" class="btn btn-reverse">Message Me</a></li>
      </ul>
      <div class="notes">
        <small>Notes: <span>{{notes}}</span></small>
      </div>
    </div>
{{else}}
<p>No Events Available</p>>
{{/each}}
    
  </section>